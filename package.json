{
  "name": "drivershub",
  "description": "An advanced Drivers Hub solution.",
  "author": "CharlesWithC <me@charlws.com> (https://charlws.com)",
  "homepage": "https://drivershub.charlws.com",
  "version": "3.3.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@hcaptcha/react-hcaptcha": "^1.8.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.134",
    "@mui/material": "^5.12.1",
    "@sentry/cli": "^2.21.1",
    "@sentry/react": "^7.74.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.5",
    "axios-retry": "^3.4.0",
    "bootstrap": "^5.2.3",
    "browserslist": "^4.22.1",
    "chart.js": "^4.4.0",
    "crypto-js": "^4.2.0",
    "electron-json-storage": "^4.6.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "i18next": "^23.7.7",
    "i18next-browser-languagedetector": "^7.2.0",
    "jsoneditor": "^9.10.2",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "moment-timezone": "^0.5.43",
    "ol": "^8.2.0",
    "papaparse": "^5.4.1",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^13.5.0",
    "react-markdown": "^8.0.7",
    "react-ol-wrapper": "^0.2.9",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.4",
    "readline-sync": "^1.4.10",
    "simplebar-react": "^3.2.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && bash ./scripts/browser-build.sh && bash -c \"find ./build -type f -name '*.map' -exec rm {} \\;\"",
    "build:dev": "react-scripts build && bash ./scripts/browser-build.sh",
    "build:electron": "react-scripts build && bash ./scripts/electron-build.sh && bash -c \"find ./build -type f -name '*.map' -exec rm {} \\;\"",
    "build:electron:dev": "react-scripts build && bash ./scripts/electron-build.sh",
    "build:serve": "react-scripts build && serve -s build --port 3001",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron:build": "electron-builder",
    "electron-pack": "build --em.main=build/electron.js",
    "sentry:sourcemaps": "npx sentry-cli sourcemaps inject --org charlws --project chub-frontend-v3 ./build && npx sentry-cli sourcemaps upload --org charlws --project chub-frontend-v3 ./build"
  },
  "main": "public/electron.js",
  "build": {
    "appId": "com.charlws.drivershub",
    "productName": "Drivers Hub by CHub",
    "artifactName": "drivershub.${ext}",
    "executableName": "drivershub",
    "icon": "public/logo.png",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "electron/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "legalTrademarks": "The Drivers Hub Project (CHub)",
      "publisherName": "CharlesWithC"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "react-router-dom": "^6.11.1"
  }
}
